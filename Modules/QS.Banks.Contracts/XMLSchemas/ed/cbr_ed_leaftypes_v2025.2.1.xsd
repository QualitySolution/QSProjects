		</xs:annotation>
		<xs:restriction base="bt:IdentifierCategory">
			<xs:minLength value="8"/>
			<xs:maxLength value="11"/>
			<xs:pattern value="[A-Z]{6}[0-9A-Z]{2}([0-9A-Z]{3})?"/>
		</xs:restriction>

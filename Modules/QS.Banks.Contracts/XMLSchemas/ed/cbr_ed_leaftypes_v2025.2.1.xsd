	<xs:simpleType name="AccountNumberRUIDType">
		<xs:annotation>
			<xs:documentation>Идентификатор. Номер счета. [Лицевой счет]. Текстовый, 20 символов.</xs:documentation>
			<xs:documentation>Identifier. Account Number. [Current Account]. Text, 20 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bt:IdentifierCategory">
			<xs:pattern value="\d{5}[0-9ABCEHKMPTX]\d{14}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountNumberRUIDorBrfType">
		<xs:annotation>
			<xs:documentation>Идентификатор. Номер балансового счета второго порядка, цифровой, 5 знаков либо номер счета [Лицевой счет], текстовый, 20 символов.</xs:documentation>
			<xs:documentation>Identifier. Balance account second order number, numeric, 5 digits or account number, text, 20 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bt:IdentifierCategory">
			<xs:pattern value="\d{5}([0-9ABCEHKMPTX]\d{14}){0,}"/>
		</xs:restriction>
	</xs:simpleType>

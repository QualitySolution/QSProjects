		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(([0-1][0-9])|(2[0-3])):[0-5][0-9]:[0-5][0-9]/(([0-1][0-9])|(2[0-3])):[0-5][0-9]:[0-5][0-9]"/>

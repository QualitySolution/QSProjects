<?xml version="1.0" encoding="Windows-1251"?>
<!--Центральный Банк Российской Федерации.
Унифицированные форматы электронных банковских сообщений.

СБП

Схемы с описанием прикладных частей электронных сообщений, используемых в расчетной сети Банка России.

-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ed701="urn:cbr-ru:ed701:v2.0" xmlns:lt="urn:cbr-ru:ed:leaftypes:v2.0" targetNamespace="urn:cbr-ru:ed701:v2.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2025.2.1">
	<xs:import namespace="urn:cbr-ru:ed:leaftypes:v2.0" schemaLocation="cbr_ed_leaftypes_v2025.2.1.xsd"/>
	<xs:annotation>
		<xs:documentation>Локальные прикладные объекты</xs:documentation>
	</xs:annotation>
	<!---*********Перенесено из objects.xsd*********-->
	<xs:annotation>
		<xs:documentation>Локальные прикладные объекты, перенесенные из objects.xsd</xs:documentation>
	</xs:annotation>
	<xs:complexType name="BankFPSED">
		<xs:annotation>
			<xs:documentation>Реквизиты банка в распоряжениях.</xs:documentation>
			<xs:documentation>Bank Attributes in FTI.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="BIC" type="lt:BICRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>БИК.</xs:documentation>
				<xs:documentation>Bank, Bank Branch.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CorrespAcc" type="lt:AccountNumberRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>Номер счета банка плательщика или номер счета банка получателя.</xs:documentation>
				<xs:documentation>Payer Agent Account Number or Beneficiary Agent Account Number opened in CBR Subdivision.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ESWithMandatoryEDReceiver">
		<xs:annotation>
			<xs:documentation>Комплексный тип для электронных сообщений. Содержит обязательный реквизит EDReceiver.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ed701:ED">
				<xs:attribute name="EDReceiver" type="lt:EDDrawerIDType" use="required">
					<xs:annotation>
						<xs:documentation>Уникальный идентификатор получателя ЭС.</xs:documentation>
						<xs:documentation>EM Receiver Unique Identifier.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ED">
		<xs:annotation>
			<xs:documentation>Базовый комплексный тип для всех электронных сообщений данного пространства имен</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ed701:EDRefID">
				<xs:sequence>
					<xs:any namespace="urn:cbr-ru:dsig:v1.1" processContents="strict" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Реквизит ЭС со значением ЗК</xs:documentation>
						</xs:annotation>
					</xs:any>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EDRefID">
		<xs:annotation>
			<xs:documentation>Реквизиты призначной группы ЭС.</xs:documentation>
			<xs:documentation>EM referenced group attributes.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="EDNo" type="lt:EDNumberType" use="required">
			<xs:annotation>
				<xs:documentation>Номер ЭС в течение опердня.</xs:documentation>
				<xs:documentation>EM Sequence Number during business day.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EDDate" type="lt:DateType" use="required">
			<xs:annotation>
				<xs:documentation>Дата составления ЭС.</xs:documentation>
				<xs:documentation>EM Generation Date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EDAuthor" type="lt:EDDrawerIDType" use="required">
			<xs:annotation>
				<xs:documentation>Уникальный идентификатор составителя ЭС - УИС.</xs:documentation>
				<xs:documentation>EM Author Unique Identifier - UID.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DepartmentalInfo">
		<xs:annotation>
			<xs:documentation>Ведомственная информация (поля 101, 104-110).</xs:documentation>
			<xs:documentation>Departmental Information (fields 101, 104-110).</xs:documentation>
		</xs:annotation>
		<xs:attribute name="DrawerStatus" type="lt:DrawerStatusCodeTextType">
			<xs:annotation>
				<xs:documentation>Поле 101.</xs:documentation>
				<xs:documentation>Field 101.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CBC" type="lt:CBCIDTextType">
			<xs:annotation>
				<xs:documentation>Поле 104.</xs:documentation>
				<xs:documentation>Field 104.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OKATO" type="lt:OKATOIDTextType">
			<xs:annotation>
				<xs:documentation>Поле 105.</xs:documentation>
				<xs:documentation>Field 105.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaytReason" type="lt:TaxPaytReasonCodeTextType">
			<xs:annotation>
				<xs:documentation>Поле 106.</xs:documentation>
				<xs:documentation>Field 106.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TaxPeriod" type="lt:TaxPeriodCodeTextType">
			<xs:annotation>
				<xs:documentation>Поле 107.</xs:documentation>
				<xs:documentation>Field 107.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocNo" type="lt:TaxDocIDTextType">
			<xs:annotation>
				<xs:documentation>Поле 108.</xs:documentation>
				<xs:documentation>Field 108.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocDate" type="lt:TaxDocDateCodeTextType">
			<xs:annotation>
				<xs:documentation>Поле 109.</xs:documentation>
				<xs:documentation>Field 109.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TaxPaytKind" type="lt:TaxPaytKindCodeTextType">
			<xs:annotation>
				<xs:documentation>Поле 110.</xs:documentation>
				<xs:documentation>Field 110.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!---******************-->
	<xs:complexType name="ED701AccDocRefID">
		<xs:annotation>
			<xs:documentation>Реквизиты исходного распоряжения.</xs:documentation>
			<xs:documentation>Settlement Document Attributes.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AccDocNo" type="lt:AccountDocIDType" use="required">
			<xs:annotation>
				<xs:documentation>Номер распоряжения.</xs:documentation>
				<xs:documentation>Initial Paper Document Number.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccDocDate" type="lt:DateType" use="required">
			<xs:annotation>
				<xs:documentation>Дата составления распоряжения.</xs:documentation>
				<xs:documentation>Initial Paper Document Issue Date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- Информация о плательщике -->
